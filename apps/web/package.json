{
  "name": "@sg-food-guide/web",
  "private": true,
  "type": "module",
  "version": "0.0.1",
  "imports": {
    "#/*": "./src/*"
  },
  "scripts": {
    "transit": "echo transit",
    "dev": "vite dev --force",
    "routes:gen": "bun scripts/generate-route-tree.ts",
    "build": "vite build",
    "preview": "vite preview",
    "typecheck": "bun run routes:gen && tsc --noEmit",
    "lint:check": "bun run routes:gen && oxlint ./src -c ../../configs/oxlint-config/oxlintrc.react.json --quiet --type-aware",
    "lint:fix": "bun run routes:gen && oxlint ./src -c ../../configs/oxlint-config/oxlintrc.react.json --fix --type-aware",
    "test": "vitest run --passWithNoTests",
    "track:sheet": "bun scripts/track-food-guide-sheet.ts",
    "populate:youtube-ids": "bun scripts/populate-youtube-ids.ts",
    "check:ratings": "bun scripts/validate-ratings.mjs",
    "d1:migrate:local": "wrangler d1 execute sg-food-guide-stalls --local --file ./data/d1/migrations/0001_init.sql",
    "d1:migrate:remote": "wrangler d1 execute sg-food-guide-stalls --remote --file ./data/d1/migrations/0001_init.sql",
    "sync:stalls:api": "curl -sS -X POST 'http://127.0.0.1:3000/api/sync/stalls?mode=dry-run'",
    "sync:stalls:scheduled:dev": "wrangler dev --test-scheduled --port 8787",
    "check": "bun run check:ratings && bun run lint:check && bun run typecheck",
    "ci": "bun run check && bun run test && bun run build",
    "deploy": "bun run build && wrangler deploy"
  },
  "dependencies": {
    "@cloudflare/vite-plugin": "catalog:api",
    "@fontsource-variable/bricolage-grotesque": "^5.2.10",
    "@fontsource-variable/dm-sans": "^5.2.8",
    "@fontsource-variable/jetbrains-mono": "^5.2.8",
    "@sg-food-guide/toolkit": "workspace:*",
    "@sg-food-guide/ui": "workspace:*",
    "@tailwindcss/vite": "catalog:ui",
    "@tanstack/react-devtools": "catalog:tanstack",
    "@tanstack/react-query": "catalog:tanstack",
    "@tanstack/react-query-devtools": "catalog:tanstack",
    "@tanstack/react-router": "catalog:tanstack",
    "@tanstack/react-router-devtools": "catalog:tanstack",
    "@tanstack/react-router-ssr-query": "catalog:tanstack",
    "@tanstack/react-start": "catalog:tanstack",
    "@tanstack/start-storage-context": "catalog:tanstack",
    "@tanstack/router-plugin": "catalog:tanstack",
    "better-result": "catalog:toolkit",
    "clsx": "catalog:ui",
    "lucide-react": "catalog:ui",
    "react": "catalog:ui",
    "react-dom": "catalog:ui",
    "tailwind-merge": "catalog:ui",
    "tailwindcss": "catalog:ui",
    "zod": "catalog:toolkit"
  },
  "devDependencies": {
    "@iconify-json/ph": "^1.2.2",
    "@sg-food-guide/tsconfig": "workspace:*",
    "@tanstack/devtools-vite": "catalog:tanstack",
    "@tanstack/router-generator": "catalog:tanstack",
    "@types/node": "catalog:dev",
    "@types/react": "catalog:ui",
    "@types/react-dom": "catalog:ui",
    "@vitejs/plugin-react": "catalog:dev",
    "oxlint": "catalog:dev",
    "typescript": "catalog:dev",
    "vite": "catalog:dev",
    "vite-tsconfig-paths": "catalog:dev",
    "vitest": "catalog:dev",
    "wrangler": "catalog:api"
  }
}
