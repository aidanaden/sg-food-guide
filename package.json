{
  "name": "users-aidan-projects-sg-food-guide",
  "private": true,
  "type": "module",
  "version": "0.0.1",
  "packageManager": "bun@1.3.3",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev --filter=@sg-food-guide/web",
    "build": "turbo run build --filter=@sg-food-guide/web",
    "check:ratings": "turbo run check:ratings --filter=@sg-food-guide/web",
    "check:astro": "turbo run check:astro --filter=@sg-food-guide/web",
